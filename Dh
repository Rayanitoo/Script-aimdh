local r=function()for _,v in ipairs(game.Players:GetPlayers())do if v~=game.Players.LocalPlayer and v.Character and v.Character:FindFirstChildOfClass("Humanoid")and v.Character:FindFirstChildOfClass("Humanoid").Health>0and v.Character:FindFirstChild("HumanoidRootPart")and v.Character:FindFirstChild("HumanoidRootPart").Position then local rayDirection=(v.Character.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.Head.Position).unit local ray=Ray.new(game.Players.LocalPlayer.Character.Head.Position,rayDirection*math.min(rayDirection.magnitude,100))local h,p=game.Workspace:FindPartOnRayWithIgnoreList(ray,{game.Players.LocalPlayer.Character},false,true)if h and h:IsDescendantOf(v.Character)then return v end end end return nil end;local m=function()local s=r()if s then getgenv().Settings.Silent.AimPart="UpperTorso"getgenv().Settings.Silent.Mode="Namecall"getgenv().Settings.Silent.FOV=30 end end;while true do m()wait(0.5)end
